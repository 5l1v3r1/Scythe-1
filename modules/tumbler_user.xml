<!-- tumbler.com - Looks for http://USER.tumblr.com -->

<module>
    <site>
        <name>tumbler.com (Username)</name>
        <url><![CDATA[http://<ACCOUNT>.tumbler.com]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>Powered by Tumblr</successmatch>
        <negativematch>We couldn't find the page you were looking for.</negativematch>
        <date>10/12/2012</date>
        <version>1</version>
        <author>Jaime Filson aka WiK</author>
        <category>social</category>
    </site>
</module>